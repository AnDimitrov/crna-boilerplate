{
  "name": "crna-boilerplate",
  "version": "0.1.0",
  "homepage": "https://github.com/AnDimitrov/crna-boilerplate",
  "private": true,
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.5",
    "standard": "^12.0.1",
    "styled-components": "^4.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public -c .storybook -o build/",
    "predeploy": "npm run build-storybook",
    "deploy": "gh-pages -d build/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@storybook/addon-a11y": "^4.0.0-rc.1",
    "@storybook/addon-actions": "^4.0.0-rc.1",
    "@storybook/addon-backgrounds": "^4.0.0-rc.1",
    "@storybook/addon-info": "^4.0.0-rc.1",
    "@storybook/addon-knobs": "^4.0.0-rc.1",
    "@storybook/addon-links": "^4.0.0-rc.1",
    "@storybook/addon-options": "^4.0.0-rc.1",
    "@storybook/addons": "^4.0.0-rc.1",
    "@storybook/react": "^4.0.0-rc.1",
    "babel-loader": "^8.0.4",
    "gh-pages": "^2.0.1"
  }
}
